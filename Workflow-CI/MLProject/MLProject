name: water_potability_project

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      data_path: {type: str, default: "water_potability_preprocessing.csv"}
      tracking_uri: {type: str, default: "http://localhost:5001"}
    command: "python modelling.py --data_path {data_path} --tracking_uri {tracking_uri}"
